{
	"compilerOptions": {
		"target": "ES2020",
		"module": "esnext",
		"lib": ["esnext", "dom"],
		"moduleResolution": "node",
		"strict": true,
		"noImplicitAny": true,
		"allowJs": true,
		"types": ["@types/node"],
		"esModuleInterop": true,
		"skipLibCheck": true
	},
	"include": ["./**/*.ts"],
	"exclude": ["node_modules"]
}